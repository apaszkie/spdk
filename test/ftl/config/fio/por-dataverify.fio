[global]
ioengine=spdk_bdev
spdk_json_conf=${FTL_JSON_CONF}
filename=${FTL_BDEV_NAME}
# SPDK cores, FTL core mask should avoid core 0
spdk_core_mask=0xfff
#${SPDK_CORE_MASK}
# CPUS allowed fio threads cannot interleave with SPDK cores
cpus_allowed=12-15
direct=1
thread=1
buffered=0
size=100%
time_based
norandommap
scramble_buffers=1
group_reporting
random_distribution=random
#random_distribution=zipf:1.2

[POR]
verify=md5
verify_pattern=0x12345678
do_verify=1
verify_dump=1
verify_backlog=1
bs=4k
numjobs=1
rw=write
#rwmixread=30
iodepth=64
runtime=3600s
