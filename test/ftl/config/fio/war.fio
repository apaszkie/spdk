[global]
ioengine=spdk_bdev
spdk_json_conf=${FTL_JSON_CONF}
filename=${FTL_BDEV_NAME}
spdk_core_mask=f
thread=1
direct=1
random_distribution=normal
norandommap
buffered=0
group_reporting
verify=crc32c
do_verify=1
verify_dump=0
verify_state_save=0
verify_fatal=1
serialize_overlap=1

[rand]
stonewall
numjobs=1
io_size=10G
rw=randwrite
bs=4k
iodepth=128
cpus_allowed=5-7
