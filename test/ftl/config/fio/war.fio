[global]
ioengine=spdk_bdev
spdk_json_conf=${FTL_JSON_CONF}
filename=${FTL_BDEV_NAME}
spdk_core_mask=f
thread=1
direct=1
random_distribution=normal
norandommap
buffered=0
group_reporting
serialize_overlap=1

[rand]
stonewall
numjobs=3
io_size=1T
rw=randrw
rwmixread=70
bs=4k
iodepth=128
randrepeat=0
cpus_allowed=5-7
